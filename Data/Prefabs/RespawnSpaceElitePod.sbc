int batteCnt = batteries.Count(); 
 
if (reactCnt &gt; 0) got++; 
if (solarCnt &gt; 0) got++; 
if (batteCnt &gt; 0) got++; 
 
if (got &lt; 1) { 
MMLCDMgr.AddLn(panel, M.T["P6"]); 
return; 
} 
 
if (isstored) { 
ShowBatteriesInfo(panel, batteries, M.T["P7"], true, displayType); 
return; 
} 
 
string title = M.T["P8"]; 
 
if (!issummary) { 
if (reactCnt &gt; 0) 
ShowPowerOutput(panel, reactors, M.T["P9"], displayType); 
if (solarCnt &gt; 0) 
ShowSolarOutput(panel, solars, M.T["P10"], displayType); 
if (batteCnt &gt; 0) 
ShowBatteriesInfo(panel, batteries, M.T["P7"], false, displayType); 
} 
else { 
