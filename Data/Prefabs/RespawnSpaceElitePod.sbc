{ 
Blocks.AddList(col.Blocks); 
} 
 
// clear all reactors from this collection   
public void Clear() 
{ 
Blocks.Clear(); 
} 
 
// number of reactors in collection   
public int Count() 
{ 
return Blocks.Count; 
} 
